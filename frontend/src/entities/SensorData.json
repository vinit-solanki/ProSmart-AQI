    "NOx": {
      "type": "number",
      "description": "Nitrogen oxides levels"
    },
    "PM1_0": {
      "type": "number",
      "description": "PM1.0 particulate matter"
    },
    "PM2_5": {
      "type": "number",
      "description": "PM2.5 particulate matter"
    },
    "PM4_0": {
      "type": "number",
      "description": "PM4.0 particulate matter"
    },
    "PM10": {
      "type": "number",
      "description": "PM10 particulate matter"
    },
    "VOCs": {
      "type": "number",
      "description": "Volatile organic compounds"
    },
    "humidity": {
      "type": "number",
      "description": "Humidity percentage"
    },
    "temperature": {
      "type": "number",
      "description": "Temperature in Celsius"
